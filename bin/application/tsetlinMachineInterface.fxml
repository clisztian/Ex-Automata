                  <Button fx:id="newSampleButton" layoutX="842.0" layoutY="113.0" mnemonicParsing="false" onAction="#handleNewSample" onKeyPressed="#handleNewSample" prefHeight="32.0" prefWidth="123.0" text="New Sample" />
                  <Button fx:id="continuousLearning" layoutX="975.0" layoutY="113.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="114.0" text="Continuous" />
                  <Button fx:id="stopButton" layoutX="1098.0" layoutY="113.0" mnemonicParsing="false" onAction="#stopLearningButton" prefHeight="32.0" prefWidth="97.0" text="Stop" />
                  <Button fx:id="beginTestButton" layoutX="554.0" layoutY="148.0" mnemonicParsing="false" onAction="#handleTesting" prefHeight="32.0" prefWidth="80.0" text="Test" />
                  <Button fx:id="buildAutomaton" layoutX="41.0" layoutY="194.0" mnemonicParsing="false" onAction="#handleAutomatonBuild" prefHeight="32.0" prefWidth="169.0" text="Build Automaton" />
                  <CheckBox fx:id="testingOut" layoutX="842.0" layoutY="156.0" mnemonicParsing="false" text="Testting" />
               </children>
            </AnchorPane>
            <Label fx:id="TimeSeriesLabel1" layoutX="45.0" layoutY="609.0" prefHeight="26.0" prefWidth="293.0" text="Automaton Learning Parameters" textFill="#d7d5fb">
               <font>
                  <Font size="17.0" />
               </font>
               <effect>
                  <Glow level="0.6" />
               </effect>
            </Label>
            <Label fx:id="smoothnessLabel3" layoutX="45.0" layoutY="650.0" prefHeight="18.0" prefWidth="135.0" text="Clause Threshold" />
            <Label fx:id="smoothnessLabel4" layoutX="44.0" layoutY="684.0" prefHeight="18.0" prefWidth="135.0" text="Clause Feedback Rate" />
            <Slider fx:id="clauseThresholdSlider" blockIncrement="0.25" layoutX="196.0" layoutY="647.0" max="3.0" min="0.25" onKeyPressed="#handleStepsChange" onMouseDragged="#handleStepsChange" prefHeight="24.0" prefWidth="169.0" value="1.0" />
            <Slider fx:id="rateSlider" blockIncrement="0.5" layoutX="196.0" layoutY="681.0" max="10.0" onKeyPressed="#handleRateChange" onMouseDragged="#handleRateChange" prefHeight="24.0" prefWidth="169.0" value="5.0" />
