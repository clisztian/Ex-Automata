<VBox xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AutomatonMachineController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
                  <CheckMenuItem fx:id="dataInterfaceCheckbox" mnemonicParsing="false" onAction="#handleOpenDataFile" text="Data Interface Panel" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="ExplainableIO">
               <items>
                  <CheckMenuItem fx:id="globalExpCheckbox" mnemonicParsing="false" onAction="#handleGlobalExpCheckbox" text="Global Explainability" />
                  <CheckMenuItem fx:id="localExpCheckbox" mnemonicParsing="false" onAction="#handleLocalExpCheckbox" text="Local Explainability" />
               </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="843.0" prefWidth="1234.0" VBox.vgrow="ALWAYS">
      <children>
            <AnchorPane layoutX="3.0" layoutY="586.0" prefHeight="253.0" prefWidth="1228.0">
               <children>
                  <Label fx:id="smoothnessLabel31" layoutX="41.0" layoutY="131.0" prefHeight="18.0" prefWidth="135.0" text="Number Clauses" />
                  <Label fx:id="smoothnessLabel41" layoutX="40.0" layoutY="165.0" prefHeight="18.0" prefWidth="144.0" text="Automaton States" />
