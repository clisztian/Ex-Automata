                <Tab fx:id="diagnosticsTab" text="Diagnostics">
                     <content>
                        <TextFlow fx:id="diagnosticTextFlow" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                </Tab>
                  <Tab fx:id="performanceTab" text="Performance History">
                     <content>
                        <StackPane fx:id="historicalPane" prefHeight="150.0" prefWidth="200.0" />
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
