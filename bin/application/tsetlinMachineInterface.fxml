            <TextField fx:id="rateSlideText" layoutX="365.0" layoutY="680.0" prefHeight="26.0" prefWidth="52.0" text="5" />
            <TabPane layoutX="12.0" layoutY="14.0" prefHeight="579.0" prefWidth="1212.0" tabClosingPolicy="UNAVAILABLE">
