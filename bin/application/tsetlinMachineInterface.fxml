                  <Slider fx:id="numStatesSlider" blockIncrement="0.25" layoutX="192.0" layoutY="162.0" max="10.0" min="0.25" onKeyPressed="#handleNumStatesChange" onMouseDragged="#handleNumStatesChange" prefHeight="24.0" prefWidth="169.0" value="2.0" />
                  <TextField fx:id="numClausesText" layoutX="361.0" layoutY="127.0" prefHeight="26.0" prefWidth="52.0" text="1" />
                  <TextField fx:id="numStatesText" layoutX="361.0" layoutY="161.0" prefHeight="26.0" prefWidth="52.0" text="2" />
                  <Label fx:id="TimeSeriesLabel11" layoutX="459.0" layoutY="26.0" prefHeight="26.0" prefWidth="293.0" text="Training Parameters" textFill="#d7d5fb">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <effect>
                        <Glow level="0.6" />
                     </effect>
                  </Label>
                  <Label fx:id="smoothnessLabel32" layoutX="459.0" layoutY="67.0" prefHeight="18.0" prefWidth="135.0" text="Training Rounds" />
                  <Slider fx:id="traindingRoundsSlider" blockIncrement="1.0" layoutX="574.0" layoutY="64.0" max="5000.0" min="1.0" onKeyPressed="#handleNumberRounds" onMouseDragged="#handleNumberRounds" prefHeight="24.0" prefWidth="169.0" value="100.0" />
                  <TextField fx:id="traindingRoundsText" layoutX="743.0" layoutY="63.0" prefHeight="26.0" prefWidth="52.0" text="100" />
                  <Label fx:id="smoothnessLabel321" layoutX="459.0" layoutY="105.0" prefHeight="18.0" prefWidth="135.0" text="Training Set Split" />
                  <Slider fx:id="traindingSplitSlider" blockIncrement="1.0" layoutX="574.0" layoutY="102.0" min="1.0" onKeyPressed="#handleTrainSetSplit" onMouseDragged="#handleTrainSetSplit" prefHeight="24.0" prefWidth="169.0" value="50.0" />
                  <TextField fx:id="traindingRoundsText1" layoutX="743.0" layoutY="101.0" prefHeight="26.0" prefWidth="52.0" text="0.5" />
                  <Button fx:id="beginTrainingButton" layoutX="459.0" layoutY="148.0" mnemonicParsing="false" onAction="#handleTraining" prefHeight="32.0" prefWidth="80.0" text="Train" />
                  <ProgressBar fx:id="trainingProgressBar" layoutX="650.0" layoutY="152.0" prefHeight="20.0" prefWidth="144.0" progress="0.0" />
                  <Label fx:id="TimeSeriesLabel111" layoutX="833.0" layoutY="26.0" prefHeight="26.0" prefWidth="325.0" text="Real-Time Learning and Prediction" textFill="#d7d5fb">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <effect>
                        <Glow level="0.6" />
                     </effect>
                  </Label>
                  <RadioButton fx:id="learningMode" layoutX="908.0" layoutY="62.0" mnemonicParsing="false" onAction="#handleChangeMode" prefHeight="30.0" prefWidth="103.0" selected="true" text="Learning" />
